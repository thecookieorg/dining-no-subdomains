<%= simple_form_for(@product_tax) do |f| %>
  <%= f.error_notification %>

  <div class="row">
  	<div class="col-md-6">
  		<%= f.input :name %>
  	</div><!-- end col-md-6 -->
  	<div class="col-md-6">
  		<%= f.input :value %>
  	</div><!-- end col-md-6 -->
  </div>
    
    

  <div class="form-actions">
    <%= f.button :submit, class: "btn btn-success" %>
  </div>
<% end %>
